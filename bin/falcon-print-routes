#!/var/www/neurona/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'falcon==1.4.1','console_scripts','falcon-print-routes'
__requires__ = 'falcon==1.4.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('falcon==1.4.1', 'console_scripts', 'falcon-print-routes')()
    )
